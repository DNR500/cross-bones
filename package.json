{
  "name": "movies",
  "version": "0.0.0",
  "description": "movies project",
  "main": "index.js",
  "scripts": {
    "versions:check": "ncu",
    "versions:update": "ncu -u",
    "metrics": "rm -rf report/plato && plato -r -d report/plato src/public/js --eslint .eslintrc.browser.json",
    "start:webpack": "webpack-dev-server",
    "start:node": "node build/node/server.js",
    "stop:node": "pkill -f 'cross-bones-node-service'",
    "lint:js": "eslint --ext .js src/public/js/** -c .eslintrc.browser.json",
    "lint:json": "jsonlint-cli .babelrc && jsonlint-cli .eslintrc.browser.json && jsonlint-cli src/**/*.json",
    "lint": "npm run lint:json && npm run lint:js",
    "casperjs": "mocha-casperjs test/casper/**/*.casper.js && npm run stop:node",
    "test:karma": "karma start",
    "test:casper": "npm run start:node & npm run casperjs",
    "test": "npm run test:karma && npm run test:casper",
    "scss": "node-sass --source-map-embed true -o build/public/css src/public/scss",
    "autoprefixer": "postcss -u autoprefixer -r build/public/css/*",
    "compile:js:node": "babel src/node --out-dir build/node --no-babelrcn --source-maps inline --preset es2015-node",
    "compile:js:public": "webpack -p",
    "compile:css": "npm run scss && npm run autoprefixer",
    "compile": "npm run compile:js:node && npm run compile:js:public && npm run compile:css",
    "build": "npm run lint && npm run test:karma && npm run compile && npm run test:casper",
    "watch": "webpack --watch & node-sass --source-map-embed true -o build/css src/public/scss --watch"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 5.7.0",
    "npm": ">= 3.6.0"
  },
  "author": "Nathan Richards <nathan.richards.500@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.11.1",
    "bootstrap-sass": "^3.3.6",
    "casper-chai": "^0.3.0",
    "casperjs": "^1.1.3",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "es5-shim": "^4.5.9",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.3.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jsonlint-cli": "^1.0.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-es6-shim": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.2",
    "mocha-casperjs": "^0.5.7",
    "node-sass": "^3.4.2",
    "npm-check-updates": "^2.8.0",
    "phantomjs-prebuilt": "^2.1.7",
    "plato": "^1.7.0",
    "postcss-cli": "^2.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "trim": "0.0.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.16.0"
  }
}
